.head {
  display: flex
}

.head van-icon {
  margin-bottom: 40rpx;
  margin-left: 40rpx;
}

.word {
  width:80%;
  text-align: center;
  margin-bottom: 40rpx;
  color: #fff;
}
.content {
  height:250rpx;width:100%;
  border-bottom: 1px solid #eee;
  display: flex
}
.content1 {
  height:160rpx;width:160rpx;
  margin: 40rpx;
}
.content1 image {
  width:100%;height:100%
}
.title {
  display: block;
  margin-top: 50rpx;
  margin-bottom: 40rpx;
  font-weight: bold
}
.text1 {
  font-size: 30rpx;
  color: #666;
  margin-right: 20rpx
}
.null image {
  width:250rpx;height:250rpx;
  position: absolute;
  left: 50%;
  transform: translateX(-50%)
}
.null text {
  margin-top:100rpx;
  display: block;
  text-align: center;
  color:#999
}