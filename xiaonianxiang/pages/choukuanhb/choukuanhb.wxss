/* pages/choukuanhb/choukuanhb.wxss */
page{
  background: #eee
}
.head {
  display: flex
}

.head van-icon {
  margin-bottom: 40rpx;
  margin-left: 40rpx;
}

.word {
  width:80%;
  text-align: center;
  margin-bottom: 40rpx;
  color: #fff;
}

.text {
  display: block;
  font-size: 28rpx;
  color: #aaa;
  width:100%;
  margin: 20rpx 0 20rpx 20rpx;
}
.list{
  width:100%;
  height: 200rpx;
  background: #fff;
  font-size: 35rpx;
  margin-bottom: 20rpx;
  position: relative;
}
.list view{
  margin-left: 20rpx;
  margin-top: 12rpx;
}
.list image{
  width: 30px;
  height: 30px;
  position: absolute;
  right: 30px;
  top: 50%;
  transform: translateY(-50%)
}
.shijian{
  font-size: 28rpx;
  color: #aaa
}
.add{
  width: 100%;
}
.van-cell-text{
  font-size: 30rpx;
  color: #aaa
}
.null image {
  width:250rpx;height:250rpx;
}
.null text {
  margin-top:100rpx;
  display: block;
  text-align: center;
  color:#999
}